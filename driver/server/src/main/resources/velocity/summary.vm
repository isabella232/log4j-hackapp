<!doctype html>
<html lang="en" data-framework="intercoolerjs">
<head>
    <meta charset="utf-8">
    <title>Log4JTester</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/css/index.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>

<body>
<h1>Grid Summary</h1>
<table class="table table-sm ">
<thead class="table-dark">
<tr><th colspan="2">Config</th>
    #foreach($g in $groups.keySet())
        <th>$g</th>
    #end
</tr>
</thead>
<tbody>

#foreach($sr in $summary)
    <tr>
        <td>$sr.jv</td>
        <td>$sr.lv</td>
        #foreach($group in $groups)
          <td class="bg-$sr.getStatus($group)">$sr.getResult($group)</td>
        #end
    </tr>
#end
</tbody>
</table>

<script src="/js/vendor/jquery-1.12.4.min.js"></script>
<script src="/js/vendor/intercooler-0.9.6.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
<script src="js/wshandler.js"></script>
</body>
</html>